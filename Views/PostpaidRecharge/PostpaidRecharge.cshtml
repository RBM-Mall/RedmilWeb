<script>

    function check(browser) {
        if (browser == "prepaid") {
            window.location.href = '@Url.Content("~/Recharge/PrepaidRecharge")';
        }
        if (browser == "postpaid") {
            window.location.href = '@Url.Content("~/PostpaidRecharge/PostpaidRecharge")';
        }
    }

    $(function () {
        $("#txtMobile").keyup(function (e) {
            var MobileNum;
            var a = $('#txtMobile').attr('id');
            var MobileNum = $("#txtMobile").val();
            if (MobileNum.length < 10) {
                $("#errMobile").html("Please insert 10 digits number");
            }
            else if (MobileNum.length == 10) {

                $("#errMobile").html("");
                $("#txtCircle").val("");
                $("#txtOperator").val("");
                $("#txtAmount").val("");
                $.ajax({
                    url: "/PostpaidRecharge/GetMobileMNPDetails",
                    data: { 'MobileNumber': MobileNum },
                    type: 'POST',
                    dataType: 'json',
                    success: function (response) {
                        if (response.result == "EmptyResult") {
                            window.location.href = response.url;
                        }
                        else if (response.result == "UnExpectedStatusCode") {
                            window.location.href = response.url;
                        }
                        else if (response.result == "RedirectToException") {
                            window.location.href = response.url;
                        }
                        else {
                            $("#txtCircle").val(response.currentLocation);
                            $("#txtOperator").val(response.currentOperator);
                            $("#operatordropdownlist").html($('<option/>', { value: response.currentOperator, text: response.currentOperator }));
                            if (response.currentOperator == "India") {
                                document.getElementById('divplans').style.display = "block";
                                var message = 'Sorry! No Records Found'
                                $("#output").html(message).css({ 'color': 'red', 'font-size': '150%', 'text-align': 'center', 'font-weight': 'bold' });
                            }
                        }
                    },
                    failure: function (response) {
                        alert(response.responseText);
                    },
                    error: function (response) {
                        alert(response.responseText);
                    }

                });
            }
        });
    });

    function bindAmountInput() {
        $(document).ready(function () {
            var amount = document.getElementById("txtAmount").value;
            $.ajax({
                type: "POST",
                url: "/PostpaidRecharge/GetBalance",
                data: {},
                type: 'POST',
                dataType: 'json',
                success: function (response) {
                    debugger;
                    if (response.result == "EmptyResult") {
                        window.location.href = response.url;
                    }
                    else if (response.result == "UnExpectedStatusCode") {
                        window.location.href = response.url;
                    }
                    else if (response.result == "RedirectToException") {
                        window.location.href = response.url;
                    }
                    else {
                        document.getElementById("lblMainbal").innerHTML = response[0].mainBal;
                        document.getElementById("lblAdbal").innerHTML = response[0].adBal;
                        document.getElementById("lblamount").innerHTML ="₹"+ amount;
                    }
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
    }

    $("#myModalfinal").on('show.bs.modal', function (e) {
        $("#myModal").modal("hide");
    });

    $('#btnCloseModal').click(function () {
        window.location.href = '@Url.Content("~/PostpaidRecharge/PostpaidRecharge")';
