        var apiKey = $("#Department_Position").val();
        //var value=apiKey.value;
        //var apiKey = $("#Department_Position").val();
        mobileno = $("#mobileno").val();
        debugger;
        if (rdbvalue == null || rdbvalue == "" || rdbvalue == undefined) {
            document.getElementById("errRadio").innerHTML = "Please Select Atleast One Language";
        }
        else if (apiKey == null || apiKey == "") {
            document.getElementById("errDepartent").innerHTML = "Please Select Atleast One Department";
        }
        else {
            debugger;
            document.getElementById("errRadio").innerHTML = "";
            document.getElementById("errDepartent").innerHTML = "";
            $.ajax({
                url: "/Support/IVRDurationCheck",
                data: { 'MaindepartmentName': MaindepartmentName, 'ApiKey': apiKey, 'mobileno': mobileno },
                type: 'POST',
                dataType: 'json',
                success: function (response) {

                    if (response.statuscode == "ERR" || response.statuscode == null) {
                        $("#btnmyModalfinal").trigger("click");
                        //$("#forcall").trigger("click");
                        document.getElementById("lblMsg").innerHTML = response.message;
                        document.getElementById("heading").innerHTML = "failed";
                        document.getElementById("img").innerHTML = '<i class="fa fa-times"></i>'
                        document.getElementById("icon").style.backgroundColor = '#880808';
                        document.getElementById("finaldismissbutton").style.backgroundColor = '#880808';

                    }
                    else if (response.statuscode == "TXN") {
                        debugger;
                        $("#forcall").trigger("click");
                        document.getElementById("lblMsgcall").innerHTML = response.message;
                        //document.getElementById("headingcall").innerHTML = "We will call you back within 5 minute";
                        document.getElementById("finaldismissbuttoncall").style.backgroundColor = '#82ce34';

                        $.ajax({
                            url: "/Support/Calluser",
                            data: { 'DepartmenmtName': departmentName, 'ApiKey': apiKey, 'mobileno': mobileno },
                            type: 'POST',
                            dataType: 'json',
                            success: function (response) {
                                document.getElementById("lblMsg1").innerHTML = response.message;
                                document.getElementById("heading1").innerHTML = "Success";
                                document.getElementById("img1").innerHTML = '<i class="fa fa-check"></i>'
                                document.getElementById("icon1").style.backgroundColor = '#82ce34';
                                document.getElementById("LocalLff").style.backgroundColor = '#82ce34';
                                $("#frenchisess").trigger("click");


                            }
                        });
                    }


                }
            })
        }



    });

