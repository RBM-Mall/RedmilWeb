            var ForHeading = $("#Department_Position option:selected").text();

            $.ajax({
                url: "/Support/ForHeading",
                data: { 'ForHeading': ForHeading },
                type: 'POST',
                dataType: 'json',
                success: function (response) {
                    $('#SId').html('');
                    var options = '<option value="' + 0 + '">' + "--Choose Sub Department--" + '</option>';
                    if (response.length > 0) {
                        for (var i = 0; i < response.length; i++) {

                            options += '<option value="' + response[i].department_key + '">' + response[i].department_name + '</option>';

                        }
                        $('#SId').show();
                        $('#SId').append(options);
                    }
                    else {
                        document.getElementById("SId").style.display = "none";
                    }


                }
            });

        });
    });
    var mobileno;
    $('.IVRDurationCheck').on('click', function () {
        debugger;
        var MaindepartmentName = $("#Department_Position option:selected").text();
        var lang = $("#Department_Position").val();
        var rdbvalue = $('.form-check-inputs:checked').val();
        //$('.radio1:checked').val();
        //document.getElementById("ddlViewBy");
