    function payNow() {
        var Number = $("#txtMobile").val();
        var Operator = $("#txtOperator").val();
        var Circle = $("#txtCircle").val();
        var Amount = $("#txtAmount").val();
        var Payment = $('.radio1:checked').val();
        if (Payment != undefined && Payment != '') {
            $.ajax({
                type: "POST",
                url: "/Recharge/PrepaidRecharge",
                data: { 'Number': Number, 'Operator': Operator, 'Circle': Circle, 'Amount': Amount, 'Payment': Payment },
                type: 'POST',
                dataType: 'json',
                success: function (response) {
                    if (response.result == "EmptyResult") {
                        window.location.href = response.url;
                    }
                    else if (response.result == "UnExpectedStatusCode") {
                        window.location.href = response.url;
                    }
                    else if (response.result == "RedirectToException") {
                        window.location.href = response.url;
                    }
                    else if (response.statuscode == "TXN") {
                        document.getElementById("lblMsg").innerHTML = response.message;
                        document.getElementById("heading").innerHTML = "Success";
                        document.getElementById("img").innerHTML = '<i class="fa fa-check"></i>'
                        document.getElementById("icon").style.backgroundColor = '#82ce34';
                        document.getElementById("lblPrice").innerHTML = 'Amount: ₹' + " " + response.data.amount;
                        document.getElementById("lblTxnId").innerHTML = 'Txn ID: ' + " " + response.data.id;
                        document.getElementById("lblMobile").innerHTML = 'Mobile No.:' + " " + response.data.mobileno;
                        var datetime = response.data.reqdate;
                        date = datetime.split('T')[0];
                        time = datetime.split('T')[1];
                        const timeString = time;

                        const timeString12hr = new Date('1970-01-01T' + timeString + 'Z')
                            .toLocaleTimeString('en-US',
                                { timeZone: 'UTC', hour12: true, hour: 'numeric', minute: 'numeric' }
                            );
                        document.getElementById("lblDate").innerHTML = 'Date :' + " " + date;
                        document.getElementById("lblTime").innerHTML = 'Time :' + " " + timeString12hr;
                        document.getElementById("lblOperatorName").innerHTML = 'Operator Name:' + " " + $("#txtOperator").val();
                        document.getElementById("lblOperatorRefNum").innerHTML = 'Operator Reference Number:' + " " + response.data.tRefId;;
                        document.getElementById("finaldismissbutton").style.backgroundColor = '#82ce34'
                        $("#btnmyModalfinal").trigger("click");
                        $("#myModal").hide();

                    }
                    else if (response.statuscode == "ERR") {
                        document.getElementById("lblMsg").innerHTML = response.message;
                        document.getElementById("heading").innerHTML = "Failed";
                        document.getElementById("img").innerHTML = '<i class="fa fa-times"></i>'
                        document.getElementById("icon").style.backgroundColor = '#880808';
                        document.getElementById("lblPrice").style.color = '#880808'
                        document.getElementById("lblPrice").innerHTML = 'Amount: ' + " " + $("#txtAmount").val();

                        if (response.data.id === undefined || response.data.id === null || response.data.id === 0) {
                            document.getElementById("lblTxnId").innerHTML = 'Txn ID: ' + " " + 'NA';
                        }
                        else {
                            document.getElementById("lblTxnId").innerHTML = 'Txn ID: ' + " " + response.data.id;
                        }
                        if (response.data.mobileno === undefined || response.data.mobileno === null || response.data.mobileno === 0) {
                            document.getElementById("lblMobile").innerHTML = 'Mobile No.:' + " " + 'NA';
                        }
                        else {
                            document.getElementById("lblMobile").innerHTML = 'Mobile No.:' + " " + response.data.mobileno;
                        }
                        var datetime = response.data.reqdate;
                        date = datetime.split('T')[0];
                        time = datetime.split('T')[1];
                        const timeString = time;

                        const timeString12hr = new Date('1970-01-01T' + timeString + 'Z')
                            .toLocaleTimeString('en-US',
                                { timeZone: 'UTC', hour12: true, hour: 'numeric', minute: 'numeric' }
                            );
                        if (response.data.reqdate === undefined || response.data.reqdate === null || response.data.reqdate === '0001-01-01T00:00:00') {
                            document.getElementById("lblDate").innerHTML = 'Date :' + " " + 'NA';
                        }
                        else {
                            document.getElementById("lblDate").innerHTML = 'Date :' + " " + date;
                        }
                        if (response.data.reqdate === undefined || response.data.reqdate === null || response.data.reqdate === '0001-01-01T00:00:00') {
                            document.getElementById("lblTime").innerHTML = 'Time :' + " " + 'NA';
                        }
                        else {
                            document.getElementById("lblTime").innerHTML = 'Time :' + " " + timeString12hr;
                        }
                        document.getElementById("lblOperatorName").innerHTML = 'Operator Name:' + " " + $("#txtOperator").val();
                        document.getElementById("lblOperatorRefNum").innerHTML = 'Operator Reference Number:' + " " + "NA";
                        document.getElementById("finaldismissbutton").style.backgroundColor = '#880808';
                        $("#btnmyModalfinal").trigger("click");
                        $("#myModal").hide();

                    }
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        }
        else {
            document.getElementById('wallet').innerHTML = "Please Select Atleast One Payment Method";
            return false;
        }
    }

