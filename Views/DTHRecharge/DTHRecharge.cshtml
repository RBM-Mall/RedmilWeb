                                                    <input id="txtCustomerId" autocomplete="off" asp-for="Circle" type="tel" maxlength="15" required="required" class="txt-input js-input-mobile txt-input-text" placeholder="Customer Id" />
                                                    <span id="errId" class="errorMsg"></span>

                                                </div>
                                                <div class="form-group-recharge">
                                                    <input id="txtAmount" autocomplete="off" asp-for="Circle" type="tel" maxlength="10" required="required" class="txt-input js-input-mobile txt-input-text" placeholder="Amount" />
                                                    <span id="errAmount" class="errorMsg"></span>

                                                </div>

