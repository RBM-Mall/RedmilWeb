<!-- Modal For ServicesComing-->

<input type="button" id="btnPSARegistrationModel" style="display:none" data-target="#modalServiceComing" data-toggle="modal" />
<div class="modal fade" id="modalServiceComing" role="dialog" tabindex="-1" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    @*<div class="modal fade" id="modalServiceComing" role="dialog">*@
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header modal-header-recharge">
                <a data-dismiss="modal" class="close close_cross">&times;</a>
            </div>
            <div class="modal-body" style="background-color:white;">
                <div class="row">
                    <div class="col-sm-12" style="text-align:center">
                        <h3 id="heading" style="text-align:center;font-weight:700;"></h3>
                        <label id="lblDataMessage"></label>
                    </div>
                </div>
            </div>
            <div class="modal-footer div-pay-row">
                <div class="row div-pay-row">
                    <div class="col-sm-4 final-pay-button">
                        <button type="button" id="btnPayNow" data-dismiss="modal" class="btn-process-recharge1 btn-process-recharge2 btn-process-recharge3">Done</button>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>
<script>
    $("#btnPSARegistration").click(function () {
        $.ajax({
            type: "POST",
            url: "/MyBusinessReports/PSARegistration",
            data: {},
            type: 'POST',
            dataType: 'json',
            success: function (response) {
                debugger;
                if (response.status == "Registered") {
                    document.getElementById("heading").innerHTML = response.status;
                    document.getElementById("lblDataMessage").innerHTML = "You are successfully register as PSA with UTI.";
                    $("#btnPSARegistrationModel").trigger("click");

                }
                else {
                    alert("Hello");

                }
            }
        });
    });
</script>