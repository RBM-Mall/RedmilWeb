    function impstransfer() {

        Amount = $('#TextImps').val();
        ServiceId = $('#SID').text();
        OpId = $('#OID').text();
        if (Amount.trim() == "") {
            $('#Text_Imps').text('Please Enter Amount');
            $('#Text_Imps').css('color', 'red');
            $('#Text_Imps').css('margin-left', '20px');
            $('#Text_Imps').focus();
        }
        else {
            $.ajax({
                url: "/Passbook/ImpstransferAmount",
                data: { 'Amount': Amount, 'ServiceId': ServiceId, 'OpId': OpId },
                type: 'POST',
                dataType: 'json',
                success: function (data) {
                    if (data.result == "EmptyResult") {
                        window.location.href = data.url;
                    }
                    else if (data.result == "UnExpectedStatusCode") {
                        window.location.href = data.url;
                    }
                    else if (data.result == "RedirectToException") {
                        window.location.href = data.url;
                    }
                    else {
                        ActualAmount = data.actualAmount;
                        Charge = data.charge;
                        Mode = data.mode;

                        $('#TextImps').val("");
                        if (data.statuscode == "ERR") {
                            $("#ModalForErrorsss").trigger("click");
                            $("#lblMsgERRData").text(data.message)
                        }
                        else {
                            $("#ModalForPrceed").trigger("click");
                            $("#ActualAmount").text(ActualAmount);
                        }
                    }


                }
            })
        }
